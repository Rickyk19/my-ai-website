{
  "version": 2,
  "env": {
    "CI": "false"
  },
  "builds": [
    {
      "src": "billion-hopes/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "billion-hopes/build"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}