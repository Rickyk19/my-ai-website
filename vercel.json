{
  "buildCommand": "npm install --legacy-peer-deps && npm run build",
  "env": {
    "CI": "false",
    "GENERATE_SOURCEMAP": "false",
    "DISABLE_ESLINT_PLUGIN": "true",
    "NPM_CONFIG_LEGACY_PEER_DEPS": "true"
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}