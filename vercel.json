{
  "version": 2,
  "builds": [
    {
      "src": "billion-hopes/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "env": {
    "CI": "false",
    "DISABLE_ESLINT_PLUGIN": "true",
    "VERCEL_FORCE_NO_BUILD_CACHE": "1"
  }
}